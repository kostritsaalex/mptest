{
  "name": "afbora/oauth2-linkedin-openid",
  "description": "LinkedIn OAuth 2.0 Client Provider for The PHP League OAuth2-Client using OpenID Connect",
  "license": "MIT",
  "authors": [
    {
      "name": "Ahmet Bora",
      "email": "byybora@gmail.com",
      "homepage": "https://github.com/afbora"
    }
  ],
  "keywords": [
    "oauth",
    "oauth2",
    "client",
    "authorization",
    "authorisation",
    "linkedin",
    "openid"
  ],
  "require": {
    "league/oauth2-client": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "mockery/mockery": "~0.9",
    "squizlabs/php_codesniffer": "~2.0",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "League\\OAuth2\\Client\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "League\\OAuth2\\Client\\Test\\": "test/src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
